= content_for :subheader do
  span
    | Revisions

#revisions-container
  .controls

  .list ng-if="namespaces"
    p.revision ng-repeat="revision in namespaces"
      | {{ revision }}
    /a
      .branch
        | {{ revision.branch }}
      .time
        | {{ revision.time }}
      .user
        | {{ revision.user }}

